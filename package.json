{
  "name": "rlay",
  "version": "1.4.9",
  "description": "Relay HTTP requests through a server to your dev machine. A self hosted alternative to ngrok",
  "main": "src/server/dist/index.js",
  "engines": {
    "node": ">16.0.0"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "rebuild": "npm run clean && npm run build",
    "build": "tsc && node copyStatic.js",
    "bns": "npm run build && npm run start",
    "start": "node dist/server/src/index.js",
    "start-client": "node dist/client/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.org/cfe84/rlay"
  },
  "devDependencies": {
    "@types/command-line-usage": "^5.0.1",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/uuid": "^8.3.0",
    "@types/crypto-js": "^4.0.2",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "testdouble": "^3.16.1",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "command-line-usage": "^6.1.1",
    "dotenv": "^10.0.0",
    "socket.io-client": "^4.1.1",
    "uuid": "^8.3.2",
    "yaclip": "^1.2.1",
    "express": "^4.17.1",
    "socket.io": "^4.1.1",
    "rlay-common": "./src/common",
    "crypto-js": "^4.1.1"
  },
  "keywords": [],
  "author": "Charles Feval",
  "license": "ISC",
  "bin": {
    "rlay": "./src/client/bin/rlay",
    "rlay-server": "./src/server/bin/rlay-server"
  }
}